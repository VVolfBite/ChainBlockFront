import Footer from \"../components/dashboard/_footer.astro\";\nimport Topbar from \"../components/dashboard/_topbar.astro\";\nimport Sidenav from \"../components/dashboard/_sidenav.astro\";\nimport Scripts from \"../components/_scripts.astro\";\n\nconst title = \"AdminLTE | Dashboard v2\";\nconst path = \"../../dist\";\nconst mainPage = \"dashboard\";\nconst page = \"index2\";\n---\n\n<!doctype html>\n<html lang=\"en\">\n  <!--begin::Head-->\n  <head>\n    <Head title={title} path={path} />\n\n    <!-- apexcharts -->\n    <link\n      rel=\"stylesheet\"\n      href=\"https://cdn.jsdelivr.net/npm/apexcharts@3.37.1/dist/apexcharts.css\"\n      integrity=\"sha256-4MX+61mt9NVvvuPjUWdUdyfZfxSB1/Rf9WtqRHgG5S0=\"\n      crossorigin=\"anonymous\"\n    />\n  </head>\n\n  <body class=\"layout-fixed sidebar-expand-lg bg-body-tertiary\">\n    <!--begin::App Wrapper-->\n    <div class=\"app-wrapper\">\n      <Topbar path={path} />\n      <Sidenav path={path} mainPage={mainPage} page={page} />\n      <!--begin::App Main-->\n      <main class=\"app-main\">\n        <!--begin::App Content Header-->\n        <div class=\"app-content-header\">\n          <!--begin::Container-->\n          <div class=\"container-fluid\">\n            <!--begin::Row-->\n            <div class=\"row\">\n              <div class=\"col-sm-6\">\n                <h3 class=\"mb-0\">总览面板</h3>\n              </div>\n              <div class=\"col-sm-6\">\n                <ol class=\"breadcrumb float-sm-end\">\n                  <li class=\"breadcrumb-item\"><a href=\"#\">面板</a></li>\n                  <li class=\"breadcrumb-item active\" aria-current=\"page\">\n                    总览面板\n                  </li>\n                </ol>\n              </div>\n            </div>\n            <!--end::Row-->\n          </div>\n          <!--end::Container-->\n        </div>\n        <div class=\"app-content\">\n          <!--begin::Container-->\n          <div class=\"container-fluid\">\n            <!-- Info boxes -->\n            <div class=\"row\">\n              <div class=\"col-12 col-sm-6 col-md-3\">\n                <div class=\"info-box\">\n                  <span class=\"info-box-icon text-bg-primary shadow-sm\">\n                    <i class=\"bi bi-box-fill\"></i>\n                  </span>\n\n                  <div class=\"info-box-content\">\n                    <span class=\"info-box-text\">区块数</span>\n                    <span class=\"info-box-number\" id=\"blockNumBox\"></span>\n                  </div>\n                  <!-- /.info-box-content -->\n                </div>\n                <!-- /.info-box -->\n              </div>\n              <!-- /.col -->\n              <div class=\"col-12 col-sm-6 col-md-3\">\n                <div class=\"info-box\">\n                  <span class=\"info-box-icon text-bg-danger shadow-sm\">\n                    <i class=\"bi bi bi-clipboard2-check\"></i>\n                  </span>\n\n                  <div class=\"info-box-content\">\n                    <span class=\"info-box-text\">事务量</span>\n                    <span class=\"info-box-number\" id=\"txnNumBox\"></span>\n                  </div>\n                  <!-- /.info-box-content -->\n                </div>\n                <!-- /.info-box -->\n              </div>\n              <!-- /.col -->\n\n              <!-- fix for small devices only -->\n              <!-- <div class=\"clearfix hidden-md-up\"></div> -->\n\n              <div class=\"col-12 col-sm-6 col-md-3\">\n                <div class=\"info-box\">\n                  <span class=\"info-box-icon text-bg-success shadow-sm\">\n                    <i class=\"bi bi-gear-fill\"></i>\n                  </span>\n\n                  <div class=\"info-box-content\">\n                    <span class=\"info-box-text\">节点数</span>\n                    <span class=\"info-box-number\" id=\"nodeNumBox\"></span>\n                  </div>\n                  <!-- /.info-box-content -->\n                </div>\n                <!-- /.info-box -->\n              </div>\n              <!-- /.col -->\n              <div class=\"col-12 col-sm-6 col-md-3\">\n                <div class=\"info-box\">\n                  <span class=\"info-box-icon text-bg-warning shadow-sm\">\n                    <i class=\"bi bi-file-earmark-code\"></i>\n                  </span>\n\n                  <div class=\"info-box-content\">\n                    <span class=\"info-box-text\">链码数</span>\n                    <span class=\"info-box-number\" id=\"chainCodeNumBox\"></span>\n                  </div>\n                  <!-- /.info-box-content -->\n                </div>\n                <!-- /.info-box -->\n              </div>\n              <!-- /.col -->\n            </div>\n            <!-- /.row -->\n\n            <!--begin::Row-->\n            <div class=\"row\">\n              <div class=\"col-md-12\">\n                <div class=\"card mb-4\">\n                  <div class=\"card-header\">\n                    <h5 class=\"card-title\">运行情况</h5>\n\n                    <div class=\"card-tools\">\n                      <button\n                        type=\"button\"\n                        class=\"btn btn-tool\"\n                        data-lte-toggle=\"card-collapse\"\n                      >\n                        <i data-lte-icon=\"expand\" class=\"bi bi-plus-lg\"></i>\n                        <i data-lte-icon=\"collapse\" class=\"bi bi-dash-lg\"></i>\n                      </button>\n                      <div class=\"btn-group\">\n                        <button\n                          type=\"button\"\n                          class=\"btn btn-tool dropdown-toggle\"\n                          data-bs-toggle=\"dropdown\"\n                        >\n                          <i class=\"bi bi-wrench\"></i>\n                        </button>\n                        <div\n                          class=\"dropdown-menu dropdown-menu-end\"\n                          role=\"menu\"\n                        >\n                          <li\n                            class=\"dropdown-item\"\n                            data-chart=\"txnThroughputChart\"\n                          >\n                            事务吞吐率\n                          </li>\n                          <li\n                            class=\"dropdown-item\"\n                            data-chart=\"blockThroughputChart\"\n                          >\n                            块吞吐率\n                          </li>\n                          <li\n                            class=\"dropdown-item\"\n                            data-chart=\"txnLatencyChart\"\n                          >\n                            事务时延\n                          </li>\n                          <li\n                            class=\"dropdown-item\"\n                            data-chart=\"blockLatencyChart\"\n                          >\n                            块时延\n                          </li>\n                        </div>\n                      </div>\n                      <button\n                        type=\"button\"\n                        class=\"btn btn-tool\"\n                        data-lte-toggle=\"card-remove\"\n                      >\n                        <i class=\"bi bi-x-lg\"></i>\n                      </button>\n                    </div>\n                  </div>\n                  <!-- /.card-header -->\n                  <div class=\"card-body\">\n                    <!--begin::Row-->\n                    <div class=\"row\">\n                      <div class=\"col-md-8\">\n                        <p class=\"text-center\">\n                          <strong id=\"chartName\"></strong>\n                           (近一分钟的每5s采样)\n                        </p>\n\n                        <div id=\"latency-throughput-chart\"></div>\n                      </div>\n                      <!-- /.col -->\n                      <div class=\"col-md-4\">\n                      