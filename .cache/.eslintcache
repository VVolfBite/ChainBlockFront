ainer-->\n          <div class=\"container-fluid\">\n            <!-- Info boxes -->\n            <div class=\"row\">\n              <div class=\"col-12 col-sm-6 col-md-3\">\n                <div class=\"info-box\">\n                  <span class=\"info-box-icon text-bg-primary shadow-sm\">\n                    <i class=\"bi bi-box-fill\"></i>\n                  </span>\n\n                  <div class=\"info-box-content\">\n                    <span class=\"info-box-text\">区块数</span>\n                    <span class=\"info-box-number\" id=\"blockNumBox\"></span>\n                  </div>\n                  <!-- /.info-box-content -->\n                </div>\n                <!-- /.info-box -->\n              </div>\n              <!-- /.col -->\n              <div class=\"col-12 col-sm-6 col-md-3\">\n                <div class=\"info-box\">\n                  <span class=\"info-box-icon text-bg-danger shadow-sm\">\n                    <i class=\"bi bi bi-clipboard2-check\"></i>\n                  </span>\n\n                  <div class=\"info-box-content\">\n                    <span class=\"info-box-text\">事务量</span>\n                    <span class=\"info-box-number\" id=\"txnNumBox\"></span>\n                  </div>\n                  <!-- /.info-box-content -->\n                </div>\n                <!-- /.info-box -->\n              </div>\n              <!-- /.col -->\n\n              <!-- fix for small devices only -->\n              <!-- <div class=\"clearfix hidden-md-up\"></div> -->\n\n              <div class=\"col-12 col-sm-6 col-md-3\">\n                <div class=\"info-box\">\n                  <span class=\"info-box-icon text-bg-success shadow-sm\">\n                    <i class=\"bi bi-gear-fill\"></i>\n                  </span>\n\n                  <div class=\"info-box-content\">\n                    <span class=\"info-box-text\">节点数</span>\n                    <span class=\"info-box-number\" id=\"nodeNumBox\"></span>\n                  </div>\n                  <!-- /.info-box-content -->\n                </div>\n                <!-- /.info-box -->\n              </div>\n              <!-- /.col -->\n              <div class=\"col-12 col-sm-6 col-md-3\">\n                <div class=\"info-box\">\n                  <span class=\"info-box-icon text-bg-warning shadow-sm\">\n                    <i class=\"bi bi-file-earmark-code\"></i>\n                  </span>\n\n                  <div class=\"info-box-content\">\n                    <span class=\"info-box-text\">链码数</span>\n                    <span class=\"info-box-number\" id=\"chainCodeNumBox\"></span>\n                  </div>\n                  <!-- /.info-box-content -->\n                </div>\n                <!-- /.info-box -->\n              </div>\n              <!-- /.col -->\n            </div>\n            <!-- /.row -->\n\n            <!--begin::Row-->\n            <div class=\"row\">\n              <div class=\"col-md-12\">\n                <div class=\"card mb-4\">\n                  <div class=\"card-header\">\n                    <h5 class=\"card-title\">运行情况</h5>\n\n                    <div class=\"card-tools\">\n                      <button\n                        type=\"button\"\n                        class=\"btn btn-tool\"\n                        data-lte-toggle=\"card-collapse\"\n                      >\n                        <i data-lte-icon=\"expand\" class=\"bi bi-plus-lg\"></i>\n                        <i data-lte-icon=\"collapse\" class=\"bi bi-dash-lg\"></i>\n                      </button>\n                      <div class=\"btn-group\">\n                        <button\n                          type=\"button\"\n                          class=\"btn btn-tool dropdown-toggle\"\n                          data-bs-toggle=\"dropdown\"\n                        >\n                          <i class=\"bi bi-wrench\"></i>\n                        </button>\n                        <div\n                          class=\"dropdown-menu dropdown-menu-end\"\n                          role=\"menu\"\n                        >\n                          <li\n                            class=\"dropdown-item\"\n                            data-chart=\"txnThroughputChart\"\n                          >\n                            事务吞吐率\n                          </li>\n                          <li\n                            class=\"dropdown-item\"\n                            data-chart=\"blockThroughputChart\"\n                          >\n                            块吞吐率\n                          </li>\n                          <li\n                            class=\"dropdown-item\"\n                            data-chart=\"txnLatencyChart\"\n                          >\n                            事务时延\n                          </li>\n                          <li\n                            class=\"dropdown-item\"\n                            data-chart=\"blockLatencyChart\"\n                          >\n                            块时延\n                          </li>\n                        </div>\n                      </div>\n                      <button\n                        type=\"button\"\n                        class=\"btn btn-tool\"\n                        data-lte-toggle=\"card-remove\"\n                      >\n                        <i class=\"bi bi-x-lg\"></i>\n                      </button>\n                    </div>\n                  </div>\n                  <!-- /.card-header -->\n                  <div class=\"card-body\">\n                    <!--begin::Row-->\n                    <div class=\"row\">\n                      <div class=\"col-md-8\">\n                        <p class=\"text-center\">\n                          <strong id=\"chartName\"></strong>\n                           (近一分钟的每5s采样)\n                        </p>\n\n                        <div id=\"latency-throughput-chart\"></div>\n                      </div>\n                      <!-- /.col -->\n                      <div class=\"col-md-4\">\n                        <p class=\"text-center\">\n                          <strong>吞吐/时延总览(每5s采样)</strong>\n                        </p>\n\n                        <div class=\"progress-group\">\n                          事务吞吐率\n                          <span class=\"float-end\"\n                            ><b id=\"txnThroughputText\"></b>份</span\n                          >\n                          <div class=\"progress progress-sm\">\n                            <div\n                              id=\"txnThroughputBar\"\n                              class=\"progress-bar text-bg-primary\"\n                              style=\"width: 80%\"\n                            >\n                            </div>\n                          </div>\n                        </div>\n\n                        <!-- /.progress-group -->\n                        <div class=\"progress-group\">\n                          <span class=\"progress-text\">块吞吐率</span>\n                          <span class=\"float-end\"\n                            ><b id=\"blockThroughputText\"></b>块</span\n                          >\n                          <div class=\"progress progress-sm\">\n                            <div\n                              id=\"blockThroughputBar\"\n                              class=\"progress-bar text-bg-success\"\n                              style=\"width: 60%\"\n                            >\n                            </div>\n                          </div>\n                        </div>\n\n                        <!-- /.progress-group -->\n                        <div class=\"progress-group\">\n                          事务时延\n                          <span class=\"float-end\"\n                            ><b id=\"txnLatencyText\"></b>ms</span\n                          >\n                          <div class=\"progress progress-sm\">\n                            <div\n                              id=\"txnLatencyBar\"\n                              class=\"progress-bar text-bg-danger\"\n                              style=\"width: 75%\"\n                            >\n                            </div>\n                          </div>\n                        </div>\n\n                        <!-- /.progress-group -->\n                        <div class=\"progress-group\">\n                          块时延\n                          <span class=\"float-end\"\n                            ><b id=\"blockLatencyText\"></b>ms</span\n                          >\n                          <div class=\"progress progress-sm\">\n                            <div\n                              id=\"blockLatencyBar\"\n                              class=\"progress-bar text-bg-warning\"\n                              style=\"width: 50%\"\n                            >\n                            </div>\n                          </div>\n                        </div>\n                        <!-- /.progress-group -->\n                      </div>\n                      <!-- /.col -->\n                    </div>\n                    <!--end::Row-->\n                  </div>\n                  <!-- ./card-body -->\n                  <div class=\"card-footer\">\n                    <!--begin::Row-->\n                    <div class=\"row\">\n                      <div class=\"col-md-3 col-6\">\n                        <div class=\"text-center border-end\">\n                          <!-- InProgress<span class=\"text-success\">\n                            <i class=\"bi bi-caret-up-fill\"></i> 17%\n                          </span> -->\n                          <h5 class=\"fw-bold mb-0\">\n                            <b id=\"txnThroughputAvgText\"></b> 份/s\n                          </h5>\n                          <span class=\"text-uppercase\">平均事务吞吐率</span>\n                        </div>\n                      </div>\n\n                      <!-- /.col -->\n                      <div class=\"col-md-3 col-6\">\n                        <div class=\"text-center border-end\">\n                          <!-- <span class=\"text-info\">\n                            <i class=\"bi bi-caret-left-fill\"></i> 0%\n                          </span> -->\n                          <h5 class=\"fw-bold mb-0\">\n                            <b id=\"blockThroughputAvgText\"></b> 块/s\n                          </h5>\n                          <span class=\"text-uppercase\">平均块吞吐率</span>\n                        </div>\n                      </div>\n                      <!-- /.col -->\n                      <div class=\"col-md-3 col-6\">\n                        <div class=\"text-center border-end\">\n                          <!-- <span class=\"text-success\">\n                            <i class=\"bi bi-caret-up-fill\"></i> 20%\n                          </span> -->\n                          <h5 class=\"fw-bold mb-0\">\n                            <b id=\"txnLatencyAvgText\"></b> ms\n                          </h5>\n                          <span class=\"text-uppercase\">平均事务时延</span>\n                        </div>\n                      </div>\n                      <!-- /.col -->\n                      <div class=\"col-md-3 col-6\">\n                        <div class=\"text-center\">\n                          <!-- <span class=\"text-danger\">\n                            <i class=\"bi bi-caret-down-fill\"></i> 18%\n                          </span> -->\n                          <h5 class=\"fw-bold mb-0\">\n                            <b id=\"blockLatencyAvgText\"></b>ms\n                          </h5>\n                          <span class=\"text-uppercase\">平均块时延</span>\n                        </div>\n                      </div>\n                    </div>\n                    <!--end::Row-->\n                  </div>\n                  <!-- /.card-footer -->\n                </div>\n                <!-- /.card -->\n              </div>\n              <!-- /.col -->\n            </div>\n            <!--end::Row-->\n\n            <!--begin::Row-->\n            <div class=\"row\">\n              <!-- Start col -->\n              <div class=\"col-md-8\">\n                <!--begin::Latest Order Widget-->\n                <div class=\"card\">\n                  <div class=\"card-header\">\n                    <h3 class=\"card-title\"><strong>最新块概览</strong></h3>\n                    <div class=\"card-tools\">\n                      <button\n                        type=\"button\"\n                        class=\"btn btn-tool\"\n                        data-lte-toggle=\"card-collapse\"\n                      >\n                        <i data-lte-icon=\"expand\" class=\"bi bi-plus-lg\"></i>\n                        <i data-lte-icon=\"collapse\" class=\"bi bi-dash-lg\"></i>\n                      </button>\n                      <button\n                        type=\"button\"\n                        class=\"btn btn-tool\"\n                        data-lte-toggle=\"card-remove\"\n                      >\n                        <i class=\"bi bi-x-lg\"></i>\n                      </button>\n                    </div>\n                  </div>\n                  <!-- /.card-header -->\n                  <div class=\"card-body p-0\">\n                    <div class=\"table-responsive\">\n                      <table class=\"table m-0\">\n                        <thead>\n                          <tr>\n                            <th>块编号</th>\n                            <th>块内事务量</th>\n                            <th>块状态</th>\n                            <th>块阶段计时</th>\n                          </tr>\n                        </thead>\n                        <tbody id=\"latestBlockTable\"> </tbody>\n                      </table>\n                    </div>\n                    <!-- /.table-responsive -->\n                  </div>\n                  <!-- /.card-body -->\n                  <div class=\"card-footer clearfix\">\n                    <a\n                      href=\"javascript:void(0)\"\n                      class=\"btn btn-sm btn-primary float-start\"\n                    >\n                      添加新的事务\n                    </a>\n                    <a\n                      href=\"javascript:void(0)\"\n                      class=\"btn btn-sm btn-secondary float-end\"\n                    >\n                      查看所有事务\n                    </a>\n                  </div>\n                  <!-- /.card-footer -->\n                </div>\n                <!-- /.card -->\n              </div>\n              <!-- /.col -->\n\n              <div class=\"col-md-4\">\n                <div class=\"card mb-4\">\n                  <div class=\"card-header\">\n                    <h3 class=\"card-title\">暂定</h3>\n\n                    <div class=\"card-tools\">\n                      <button\n                        type=\"button\"\n                        class=\"btn btn-tool\"\n                        data-lte-toggle=\"card-collapse\"\n                      >\n                        <i data-lte-icon=\"expand\" class=\"bi bi-plus-lg\"></i>\n                        <i data-lte-icon=\"collapse\" class=\"bi bi-dash-lg\"></i>\n                      </button>\n                      <button\n                        type=\"button\"\n                        class=\"btn btn-tool\"\n                        data-lte-toggle=\"card-remove\"\n                      >\n                        <i class=\"bi bi-x-lg\"></i>\n                      </button>\n                    </div>\n                  </div>\n                  <!-- /.card-header -->\n                  <div class=\"card-body\">\n                    <!--begin::Row-->\n                    <div class=\"row\">\n                      <div class=\"col-12\">\n                        <div id=\"pie-chart\"></div>\n                      </div>\n                      <!-- /.col -->\n                    </div>\n                    <!--end::Row-->\n                  </div>\n                  <!-- /.card-body -->\n                  <div class=\"card-footer p-0\">\n                    <ul class=\"nav nav-pills flex-column\">\n                      <li class=\"nav-item\">\n                        <a href=\"#\" class=\"nav-link\">\n                          United States of America\n                          <span class=\"float-end text-danger\">\n                            <i class=\"bi bi-arrow-down fs-7\"></i>\n                             12%\n                          </span>\n                        </a>\n                      </li>\n                      <li class=\"nav-item\">\n                        <a href=\"#\" class=\"nav-link\">\n                          India\n                          <span class=\"float-end text-success\">\n                            <i class=\"bi bi-arrow-up fs-7\"></i> 4%\n                          </span>\n                        </a>\n                      </li>\n                      <li class=\"nav-item\">\n                        <a href=\"#\" class=\"nav-link\">\n                          China\n                          <span class=\"float-end text-info\">\n                            <i class=\"bi bi-arrow-left fs-7\"></i> 0%\n                          </span>\n                        </a>\n                      </li>\n                    </ul>\n                  </div>\n                  <!-- /.footer -->\n                </div>\n                <!-- /.card -->\n              </div>\n              <!-- /.col -->\n            </div>\n            <!--end::Row-->\n          </div>\n          <!--end::Container-->\n        </div>\n        <!--end::App Content-->\n      </main>\n      <!--end::App Main-->\n      <Footer />\n    </div>\n    <!--end::App Wrapper-->\n    <!--begin::Script-->\n    <Scripts path={path} />\n\n    <!-- OPTIONAL SCRIPTS -->\n\n    <!-- apexcharts -->\n    <script\n      src=\"https://cdn.jsdelivr.net/npm/apexcharts@3.37.1/dist/apexcharts.min.js\"\n      integrity=\"sha256-+vh8GkaU7C9/wbSLIcwq82tQ2wTf44aOHA8HlBMwRI8=\"\n      crossorigin=\"anonymous\"></script>\n    <script is:inline>\n      //-----------------------\n      // - THROUGHPUT/LATENCY CHART -\n      //-----------------------\n\n      /* apexcharts\n       * -------\n       * Here we will create a few charts using apexcharts\n       */\n      const chartData = {\n        txnThroughputChart: {\n          name: \"事务吞吐率\",\n          data: [30, 40, 35, 50, 45, 60, 55, 70, 10, 1, 2, 3, 4],\n        },\n        blockThroughputChart: {\n          name: \"块吞吐率\",\n          data: [10, 20, 15, 30, 25, 40, 35, 50, 10, 2, 4, 6, 8],\n        },\n        txnLatencyChart: {\n          name: \"事务时延\",\n          data: [5, 10, 7, 15, 12, 20, 17, 25, 10, 3, 6, 9, 12],\n        },\n        blockLatencyChart: {\n          name: \"块时延\",\n          data: [8, 16, 12, 24, 20, 32, 28, 40, 10, 10, 10, 0, 10],\n        },\n      };\n      const chartOptions = {\n        series: [\n          //默认使用事务吞吐率\n          {\n            name: chartData.txnThroughputChart.name,\n            data: chartData.txnThroughputChart.data,\n          },\n        ],\n        chart: {\n          height: 180,\n          type: \"area\",\n          toolbar: {\n            show: false,\n          },\n        },\n        legend: {\n          show: false,\n        },\n        dataLabels: {\n          enabled: false,\n        },\n        stroke: {\n          curve: \"smooth\",\n        },\n        xaxis: {\n          categories: [\n            \"60s\",\n            \"55s\",\n            \"50s\",\n            \"45s\",\n            \"40s\",\n            \"35s\",\n            \"30s\",\n            \"25s\",\n            \"20s\",\n            \"15s\",\n            \"10s\",\n            \"5s\",\n            \"0s\",\n          ],\n        },\n      };\n      const chartName = document.getElementById(\"chartName\");\n      chartName.innerHTML = \"事务吞吐率\";\n      const latencyThroughputChart = new ApexCharts(\n        document.querySelector(\"#latency-throughput-chart\"),\n        chartOptions,\n      );\n      latencyThroughputChart.render();\n      //设置切换监听函数并切换曲线图\n      document.querySelectorAll(\".dropdown-item\").forEach((item) => {\n        item.addEventListener(\"click\", () => {\n          const selectedChart = item.getAttribute(\"data-chart\");\n          const chartConfig = chartData[selectedChart];\n          latencyThroughputChart.updateSeries([\n            {\n              name: chartConfig.name,\n              data: chartConfig.data,\n            },\n          ]);\n          const chartName = document.getElementById(\"chartName\");\n          chartName.innerHTML = chartConfig.name;\n        });\n      });\n\n      //---------------------------\n      // - END THROUGHPUT/LATENCY CHART -\n      //---------------------------\n\n      //-------------------------\n      //   Create Timer Chart\n      //-------------------------\n\n      function createSparklineChart(selector, data) {\n        const options = {\n          series: [{ data }],\n          xaxis: {\n            categories: [\"发布\", \"排队\", \"提交\", \"确认\"],\n          },\n          chart: {\n            type: \"line\",\n            width: 150,\n            height: 30,\n            sparkline: {\n              enabled: true,\n            },\n          },\n          colors: [\"var(--bs-primary)\"],\n          stroke: {\n            width: 2,\n          },\n          tooltip: {\n            fixed: {\n              enabled: false,\n            },\n            x: {\n              show: true,\n            },\n            y: {\n              title: {\n                formatter: function (seriesName) {\n                  return \"计时:\";\n                },\n              },\n              formatter: function (value) {\n                return value + \"ms\";\n              },\n            },\n            marker: {\n              show: false,\n            },\n          },\n        };\n\n        const chart = new ApexCharts(document.querySelector(selector), options);\n        chart.render();\n      }\n\n      //-------------------------\n      //   END CREATE Timer Chart\n      //-------------------------\n\n      //-----------------------\n      // - Normal Info Box-\n      //-----------------------\n\n      function requestNormalInfo() {\n        fetch(\"http://localhost:8000/NormalInfo\")\n          .then((response) => {\n            if (response.ok) {\n              return response.json();\n            } else {\n              throw new Error(\"Failed to fetch data\");\n            }\n          })\n          .then((data) => {\n            const { blockNum, txnNum, nodeNum, chainCodeNum } = data;\n\n            console.log(data);\n\n            //更新盒子信息\n            const blockNumBox = document.getElementById(\"blockNumBox\");\n            blockNumBox.innerHTML = blockNum;\n            const txnNumBox = document.getElementById(\"txnNumBox\");\n            txnNumBox.innerHTML = txnNum;\n            const nodeNumBox = document.getElementById(\"nodeNumBox\");\n            nodeNumBox.innerHTML = nodeNum;\n            const chainCodeNumBox = document.getElementById(\"chainCodeNumBox\");\n            chainCodeNumBox.innerHTML = chainCodeNum;\n          })\n          .catch((error) => {\n            console.error(\"Error:\", error);\n          });\n      }\n      setInterval(requestNormalInfo, 5000);\n\n      //-----------------------\n      // - End Normal Info Box-\n      //-----------------------\n\n      //-----------------------\n      // - Running Info Box-\n      //-----------------------\n      function requestRunningInfo() {\n        fetch(\"http://localhost:8000/RunningInfo\")\n          .then((response) => {\n            if (response.ok) {\n              return response.json(); // 解析 JSON 响应\n            } else {\n              throw new Error(\"Failed to fetch data\");\n            }\n          })\n          .then((data) => {\n   